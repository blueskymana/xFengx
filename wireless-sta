
config wifi-device 'wifi0'
	option channel 'auto'
	option hwmode '11ng'
	option txpower '17'
	option htmode 'HT20'

config wifi-iface
	option device 'wifi0'
	option network 'lan'
	option mode 'ap'
	option ssid 'nexfi-ap0'
	option encryption 'psk-mixed'
	option key '12345678'

config wifi-device 'wifi1'
	option channel 'auto'
	option hwmode '11ac'
	option txpower '17'
	option htmode 'HT20'

config wifi-iface
	option device 'wifi1'
	option mode 'sta'
	option ssid 'nexfi-br0'
	option encryption 'psk-mixed'
	option key '048C159D'

